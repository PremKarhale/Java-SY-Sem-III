package filehandling;
import java.io.File;

public class filehandling {
	public static void main(String[] args) {
	 File myfile=new File("chwh111.txt");
	 try {
		 myfile.createNewFile();
	 }
	 catch(Exception e) {
		 System.out.println("my file is not created ");
		 e.printStackTrace();
 
	 }
	 
	}

}
